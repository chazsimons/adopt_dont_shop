<p><%= form_with url: "/applications", method: :post, local: true do |form| %>
<%= form.label :name, "Name:" %>
<%= form.text_field :name %><br>
<%= form.label :street_address, "Street Address:" %>
<%= form.text_field :street_address %><br>
<%= form.label :city, "City:" %>
<%= form.text_field :city %><br>
<%= form.label :state, "State:"%>
<%= form.text_field :state %><br>
<%= form.label :zip_code, "Zip Code:"%>
<%= form.text_field :zip_code %><br>
<%= form.hidden_field :status, value: 'In Progress' %>
<%= form.submit "Submit" %></p>
<% end %>
